source "C" from "cbase.h" (
    "packDouble"
  , "unpackDouble"
  , "packInt"
  , "unpackInt"
  , "packString"
  , "unpackString"
);

packDouble C :: packs => Double -> JSON;
unpackDouble C :: unpacks => JSON -> Double;

packInt C :: packs => Int -> JSON;
unpackInt C :: unpacks => JSON -> Int;

packString C :: packs => String -> JSON;
unpackString C :: unpacks => JSON -> String;
